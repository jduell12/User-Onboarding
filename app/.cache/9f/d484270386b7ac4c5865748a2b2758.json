olumn":12}},{"generated":{"line":76,"column":16},"source":"Components/Form.js","original":{"line":9,"column":8}},{"generated":{"line":76,"column":17},"source":"Components/Form.js","original":{"line":8,"column":4}},{"generated":{"line":77,"column":0},"source":"Components/Form.js","original":{"line":86,"column":1}}],"sources":{"Components/Form.js":"import React from 'react'\nimport {StyledForm, StyledInnerForm, StyledButton, StyledErrorDiv} from './StyledForm'\n\n\nexport default function Form(props){\n    const {values, onInputChange, onCheckboxChange, onSubmit, disabled, errors} = props\n\n    return (\n        <div>\n            <StyledForm onSubmit={onSubmit}>\n                <h2>Create a User</h2>\n                    <StyledErrorDiv>\n                    <div>{errors.name}</div>\n                    <div>{errors.email}</div>\n                    <div>{errors.password}</div>\n                    <div>{errors.role}</div>\n                    <div>{errors.terms}</div>\n                </StyledErrorDiv>\n                <StyledInnerForm>\n                    <label htmlFor='nameInput'>\n                        Name: &nbsp;\n                        <input \n                            id='nameInput'\n                            type='text' \n                            value={values.name}\n                            onChange={onInputChange}\n                            name='name'\n                        />\n                    </label>\n\n                    <label htmlFor='emailInput'>\n                        Email: &nbsp;\n                        <input\n                            type='text'\n                            id='emailInput'\n                            name='email'\n                            value={values.email}\n                            onChange={onInputChange}\n                        />\n                    </label>\n\n                    <label htmlFor='passwordInput'>\n                        Password: &nbsp;\n                        <input \n                            type='password'\n                            id='passwordInput'\n                            name='password'\n                            value={values.password}\n                            onChange={onInputChange}\n                        />\n                    </label>\n\n                    <label>\n                        Role &nbsp;\n                        <select\n                            onChange={onInputChange}\n                            value={values.role}\n                            name='role'\n                        >\n                            <option value=''>-- Select a Role --</option>\n                            <option value='Gamer'>Gamer</option>\n                            <option value='Student'>Student</option>\n                            <option value='Instructor'>Instructor</option>\n                            <option value='Hobbiest'>Hobbiest</option>\n                            <option value='Developer'>Developer</option>\n                        </select>\n                    </label>\n                    \n                    <label htmlFor='checkboxInput'>\n                        Terms of Service &nbsp;\n                        <input \n                            type='checkbox'\n                            id='checkboxInput'\n                            name='terms'\n                            value={values.terms}\n                            onChange={onCheckboxChange}\n                            checked={values.terms}\n                        />\n                        \n                    </label>\n                    <StyledButton disabled={disabled}>Submit</StyledButton>\n                </StyledInnerForm>\n            </StyledForm>\n        </div>\n    )\n}\n"},"lineCount":null}},"error":null,"hash":"c1770ad91b28512f358cc1ee78302681","cacheData":{"env":{}}}